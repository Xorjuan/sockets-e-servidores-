def convert_hex(hex):
    print(int(hex, 16))  # converte de str para int em base 16


def convert_bin(binario):
    print(bin(binario))  # converte em int -> bin


convert_hex(str(input("digite um valor para a convers達o em hexa:")))
convert_bin(int(input("digite um valor para a convers達o em binario:")))

